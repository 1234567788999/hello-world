//Theme Set up:
function pairgiver_theme_setup() {
    /*
     * This theme uses a custom image size for featured images, displayed on
     * "standard" posts and pages.
     */
  	add_theme_support( 'custom-header' ); 
  	add_theme_support( 'custom-background' );
    remove_filter('the_content', 'wpautop');
  	$lang = get_template_directory_uri() . '/languages';
    load_theme_textdomain('pairgiver', $lang);
    add_theme_support( 'post-thumbnails' );
    // Adds RSS feed links to <head> for posts and comments.
    add_theme_support( 'automatic-feed-links' );
    // Switches default core markup for search form, comment form, and comments
    // to output valid HTML5.
    add_theme_support( 'title-tag' );
    add_theme_support( 'html5', array( 'search-form', 'comment-form', 'comment-list' ) );
    // This theme uses wp_nav_menu() in one location.
  	register_nav_menus( array(
      'primary' =>  esc_html__( 'Primary Navigation Menu: Chosen menu in  pages ...', 'pairgiver' ),
  	) );
      // This theme uses its own gallery styles.
}
add_action( 'after_setup_theme', 'pairgiver_theme_setup' );
if ( ! isset( $content_width ) ) $content_width = 900;
function pairgiver_theme_scripts_styles() {
  	$pairgiver_redux_demo = get_option('redux_demo');
  	$protocol = is_ssl() ? 'https' : 'http';
    wp_enqueue_style( 'bootstrap', get_template_directory_uri().'/assets/css/bootstrap.min.css');
    wp_enqueue_style( 'owl-carousel', get_template_directory_uri().'/assets/css/owl.carousel.min.css');
    wp_enqueue_style( 'animate', get_template_directory_uri().'/assets/css/animate.css');
    wp_enqueue_style( 'magnific-popup', get_template_directory_uri().'/assets/css/magnific-popup.css');
    wp_enqueue_style( 'all', get_template_directory_uri().'/assets/css/all.min.css');
    wp_enqueue_style( 'flaticon', get_template_directory_uri().'/assets/css/flaticon.css');
    wp_enqueue_style( 'font', get_template_directory_uri().'/assets/css/font.css');
    wp_enqueue_style( 'themify-icons', get_template_directory_uri().'/assets/css/themify-icons.css');
    wp_enqueue_style( 'metisMenu', get_template_directory_uri().'/assets/css/metisMenu.css');
    wp_enqueue_style( 'nice-select', get_template_directory_uri().'/assets/css/nice-select.css');
    wp_enqueue_style( 'pairgiver-slick', get_template_directory_uri().'/assets/css/slick.css');
    wp_enqueue_style( 'pairgiver-main', get_template_directory_uri().'/assets/css/main.css');
    wp_enqueue_style( 'pairgiver-style', get_stylesheet_uri(), array(), '2021-04-05' );

    if(isset($pairgiver_redux_demo['rtl']) && $pairgiver_redux_demo['rtl']==1){
    wp_enqueue_style( 'rtl', get_template_directory_uri().'/rtl.css');  }

  	if ( is_singular() && comments_open() && get_option( 'thread_comments' ) )
    wp_enqueue_script( 'comment-reply' );
    //Javascript
    wp_enqueue_script('modernizr-3.5.0', get_template_directory_uri().'/assets/js/vendor/modernizr-3.5.0.min.js',array(),false,true);
    wp_enqueue_script('jquery-1.12.4', get_template_directory_uri().'/assets/js/vendor/jquery-1.12.4.min.js',array(),false,true);
    wp_enqueue_script('popper', get_template_directory_uri().'/assets/js/popper.min.js',array(),false,true);
    wp_enqueue_script('bootstrap', get_template_directory_uri().'/assets/js/bootstrap.min.js',array(),false,true);
    wp_enqueue_script('owl-carousel', get_template_directory_uri().'/assets/js/owl.carousel.min.js',array(),false,true);
